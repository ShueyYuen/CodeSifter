  "version": "0.2.0",
  "configurations": [
    {
      "type": "node-terminal",
      "name": "Run vue build",
      "request": "launch",
      "command": "pnpm run build",
      "autoAttachChildProcesses": true,
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}/playground/vue-example",
      "env": {
        "NODE_ENV": "release"
      }
    }
  ]
}
