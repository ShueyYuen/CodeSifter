{
  "name": "code-sifter",
  "version": "1.0.0",
  "description": "A conditional compilation plugin for webpack and rollup",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShueyYuen/CodeSifter.git"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "webpack",
    "rollup-plugin",
    "loader",
    "plugin",
    "conditional",
    "compilation"
  ],
  "author": "Shuey Yuen <me@cofal.cn>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./vite": "./dist/vite.js",
    "./webpack": "./dist/webpackPlugin.js",
    "./rspack": "./dist/rspack.js",
    "./rollup": "./dist/rollup.js",
    "./rolldown": "./dist/rolldown.js",
    "./esbuild": "./dist/esbuild.js",
    "./farm": "./dist/farm.js",
    "./api": "./dist/api.js",
    "./*": "./*"
  },
  "devDependencies": {
    "@types/node": "22.13.11",
    "@types/webpack": "5.28.5",
    "rollup": "4.37.0",
    "typescript": "5.8.2",
    "webpack": "5.98.0"
  },
  "dependencies": {
    "magic-string": "0.30.17",
    "unplugin": "^2.2.2",
    "unplugin-utils": "0.2.4"
  }
}
