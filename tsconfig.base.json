{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["ESNext"],
    "module": "Preserve",
    "inlineSourceMap": true,
    "moduleResolution": "bundler",
    "declaration": true,
    "strict": true,
    "esModuleInterop": true,
    "types": ["node"],
    "skipLibCheck": true,
    "isolatedModules": true,
    "isolatedDeclarations": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "node_modules",
  ],
}
