{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "ESNext"
    ],
    "module": "Preserve",
    "moduleResolution": "bundler",
    "declaration": true,
    "moduleDetection": "force",
    "strict": true,
    "esModuleInterop": true,
    "types": [
      "node"
    ],
    "skipLibCheck": true,
    "isolatedModules": true,
    "isolatedDeclarations": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "node_modules",
  ],
}
